{
  "name": "@skbkontur/cassandra-distributed-task-queue-ui",
  "version": "1.0.0",
  "description": ".NET library implementing distributed task queue machinery using Apache Cassandra",
  "main": "./index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/cassandra-distributed-task-queue.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/cassandra-distributed-task-queue/issues"
  },
  "homepage": "https://github.com/skbkontur/cassandra-distributed-task-queue",
  "keywords": [
    "Distributed",
    "TaskQueue",
    "Cassandra"
  ],
  "author": "Ivan Burmistrov, Andrew Kostousov, Eugene Tihonov",
  "scripts": {
    "build": "run-s build:types build:clean build:tsc \"build:copy:*\" build:patch build:pack",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc --project tsconfig.prod.json",
    "build:patch": "node ./patch-version",
    "build:copy:src": "copyfiles --up 1 \"src/**/*.js\" dist",
    "build:copy:meta": "copyfiles --flat package.json \"../*.md\" ../LICENSE dist",
    "build:pack": "cd dist && npm pack",
    "build:types": "dotnet ts-gen --assembly ./../Cassandra.DistributedTaskQueue.Monitoring.TestService/bin/net6.0/SkbKontur.Cassandra.DistributedTaskQueue.Monitoring.TestService.dll --outputDir ./src/Domain/Api --nullabilityMode Pessimistic",
    "start": "vite --mode dev",
    "start:prod": "vite",
    "storybook": "sb dev -p 6006",
    "lint": "run-s \"lint:*\"",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint ./ --ext .js,.jsx,.ts,.tsx --cache --quiet",
    "test": "vitest --run --reporter dot"
  },
  "peerDependencies": {
    "@skbkontur/icons": ">=1 <2",
    "@skbkontur/react-ui": ">=2 <=4",
    "react": ">=16 <=18",
    "react-dom": ">=16 <=18",
    "react-router-dom": ">=6"
  },
  "dependencies": {
    "@skbkontur/react-stack-layout": "^1.0.3",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.29.2",
    "decimal.js": "^10.2.1",
    "lodash": "^4.17.20",
    "qs": "^6.11.2",
    "tslib": "^2.4.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@skbkontur/icons": "^1.3.0",
    "@skbkontur/react-ui": "^4.1.0",
    "@storybook/addon-actions": "^7.0.7",
    "@storybook/addons": "^7.0.7",
    "@storybook/cli": "^7.4.6",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.4.6",
    "@types/lodash": "^4.14.171",
    "@types/qs": "^6.9.8",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.15",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^3.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.24.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.11.0",
    "rimraf": "^3.0.2",
    "storybook-addon-react-router-v6": "^2.0.7",
    "typescript": "^4.3.5",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@4.0.0+sha256.6d855253732ba8d231b6cd917961654f6c8439164c962a4e355c9c58360ebe44"
}
